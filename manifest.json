{
  "manifest_version": 2,
  "name": "SatNOGS POC",
  "version": "1.0.2",
  "description": "",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
          "*://127.0.0.1/*",
          "https://network.satnogs.org/*"
      ],
      "js": ["content-script.js"]
    }
  ],
  "background": {
    "scripts": ["background-script.js"]
  },
  "options_ui": {
    "page": "settings.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    }
  },
  "permissions": ["tabs", "storage"]
}
